<% layout('layouts/new') %> 
    <h3>this is all user of your websites</h3>
    <% for(let user of users) {%>
        <div class="card">
            <div class="row">
                <div class="col-md-4">
                    <img src="data:image/png;base64," class="img-fluid" alt="Responsive image">
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <h5 class="card-title">
                            <%= user.username %> 

                        </h5>
                        <p class="card-text">
                            <%= user.email %> 
                        </p>
                        <p class="card-text">
                        <%=user.number %> 
                            </p>
                        <!-- <p class="card-text">
                          <small class="text-muted">  </small>
                        </p> -->
                        <a class="btn btn-primary" href="/user/<%=user._id%>">View:<%= user.username %> </a>

                    </div>
                </div>

            </div>



        </div>
        <% } %>   
    
